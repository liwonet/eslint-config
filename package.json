{
  "name": "@liwo/eslint-config",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.13.1",
  "description": "ESLint config by Liwo",
  "author": "Aoang <aoang@x2oe.com>",
  "license": "MIT",
  "homepage": "https://github.com/liwonet/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liwonet/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/liwonet/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build && tsx scripts/fixed-netlify.ts",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "gen": "tsx scripts/typegen.ts",
    "prepack": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@unocss/eslint-plugin": "^66.3.3",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "1.1.0",
    "@clack/prompts": "0.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint-react/eslint-plugin": "1.52.3",
    "@eslint/markdown": "7.1.0",
    "@stylistic/eslint-plugin": "5.2.2",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint": "9.32.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-flat-config-utils": "2.1.1",
    "eslint-merge-processors": "2.0.0",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-command": "3.3.1",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-import-lite": "0.3.0",
    "eslint-plugin-jsdoc": "52.0.2",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-pnpm": "1.1.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-toml": "0.12.0",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.4.0",
    "eslint-plugin-yml": "1.18.0",
    "eslint-processor-vue-blocks": "2.0.0",
    "globals": "16.3.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.1.1",
    "parse-gitignore": "2.0.0",
    "toml-eslint-parser": "0.10.0",
    "vue-eslint-parser": "10.2.0",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@liwo/eslint-config": "workspace:*",
    "@prettier/plugin-xml": "^3.4.2",
    "@types/node": "24.1.0",
    "@unocss/eslint-plugin": "^66.3.3",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^2.2.1",
    "execa": "^9.6.0",
    "lint-staged": "^16.1.2",
    "pnpm-workspace-yaml": "^1.0.0",
    "prettier-plugin-slidev": "^1.0.5",
    "react": "^19.1.0",
    "simple-git-hooks": "^2.13.0",
    "tinyglobby": "^0.2.14",
    "tsdown": "^0.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.17"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "22.17.1"
  }
}
